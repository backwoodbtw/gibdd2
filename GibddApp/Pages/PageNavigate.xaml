<Page x:Class="GibddApp.Pages.PageNavigate"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GibddApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageNavigate">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Просмотр всех записей" Click="BtnViewAll_Click" Width="200" Height="30" Margin="0,0,0,10"/>
            <Button Content="Добавить/удалить запись" Click="BtnAddDelete_Click" Width="200" Height="30"/>
        </StackPanel>
    </Grid>
</Page>
